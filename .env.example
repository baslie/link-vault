# Public keys used by the Supabase JS client on the browser
NEXT_PUBLIC_SUPABASE_URL="https://your-project-id.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="public-anon-key"

# Private keys used only on the server (never expose in the client bundle)
SUPABASE_SERVICE_ROLE_KEY="service-role-key"
SUPABASE_DB_PASSWORD="postgres-password"

# Optional: URL used by Next.js when generating absolute links
NEXT_PUBLIC_SITE_URL="http://localhost:3000"
